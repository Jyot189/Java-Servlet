<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LoginSystem</display-name>
  <servlet>
	  <servlet-name>LoginSystemServlet</servlet-name>
	  <servlet-class>in.ac.adit.contoller.LoginSystemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>LoginSystemServlet</servlet-name>
	  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>isAuthentication</servlet-name>
  <servlet-class>in.ac.adit.contoller.isAuthentication</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>isAuthentication</servlet-name>
   <url-pattern>/authentication</url-pattern>
  </servlet-mapping>
</web-app>